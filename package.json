  "bugs": {
    "url": "https://github.com/Great0S/pazar-plus/issues"
  },
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "start:dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install:all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "test": "jest",
    "test:client": "cd client && npm test",
    "test:all": "npm run test && npm run test:client",
    "lint": "eslint server/ --ext .js",
    "lint:fix": "eslint server/ --ext .js --fix",
    "backup:db": "cp database.sqlite database.backup.sqlite",
    "setup": "npm run clean:duplicates && npm run clean:dev-files && npm install && cd client && npm install",
    "start:fresh": "npm run cleanup:all && npm install && npm run server",
    "start:final": "npm run cleanup:all && npm run fix:imports && npm run fix:controllers && npm run fix:imports:final && npm run fix:all-imports && npm run fix:websocket && npm run fix:all-services && npm run server",
    "start:prod": "NODE_ENV=production node server/server.js",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:restart": "pm2 restart pazar-plus",
    "pm2:stop": "pm2 stop pazar-plus",
    "pm2:logs": "pm2 logs pazar-plus"
  },
